{
  "name": "@graphql-codegen/typescript-graceful-graphql-interfaces",
  "version": "0.0.7",
  "type": "module",
  "description": "create helper functions for graphql interfaces",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-code-generator-community.git",
    "directory": "packages/plugins/typescript/graceful-graphql-interfaces"
  },
  "author": "Jonas Richter",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman --config ../../../../jest.config.js --forceExit"
  },
  "peerDependencies": {
    "@graphql-codegen/cli": "^4.0.0",
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/visitor-plugin-common": "4.0.1",
    "ts-morph": "^19.0.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@graphql-codegen/import-types-preset": "2.2.6",
    "@graphql-codegen/testing": "1.18.0",
    "@types/lodash": "4.14.195"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
